fpath=(~/.completions /usr/local/brew/share/zsh-completions /usr/local/brew/share/zsh/functions /usr/local/brew/share/zsh/site-functions)

lockfile -1 -r5 ~/.zshrc-lock
source ~/.bundles/gpg-agent
source ~/.bundles/rbenv
rm -f ~/.zshrc-lock

alias ls='ls -G'

alias c='hss'
alias ccp='scp -S hss'
export HSS_PASS='ssh'

alias g='gist -c'

alias s='ssh'

alias gpom='git push origin master'
alias gppm='git push public master'
alias gpam='git push public master ; git push origin master'

alias keybase='keybase --homedir /Volumes/akerl-vault'

export SSL_CERT_FILE=/Users/akerl/.sslcerts

