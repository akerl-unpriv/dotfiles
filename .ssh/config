Host *.a-rwx.org
    HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com
    KexAlgorithms curve25519-sha256@libssh.org
    Ciphers chacha20-poly1305@openssh.com

Host github.com
    ControlMaster no

Host music
    Hostname miro.a-rwx.org
    AddressFamily inet
    Port 20

Host irclogs
    Hostname miro.a-rwx.org
    AddressFamily inet
    Port 20

Host *
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h:%p
    ControlPersist 1h

